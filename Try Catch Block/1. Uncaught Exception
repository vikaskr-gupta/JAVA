class A1_Uncaught_Exceptions {
    public static void main(String[] args)
    {
        int d = 0;
        int a = 10 / d; // EXCEPTION OCCUR
        System.out.println(a);
        
        // SHOWING ERROR!!!
        // java.lang.ArithmeticException: / by zero
        // at B1_Uncaught_Exceptions.main(B1_Uncaught_Exceptions.java:5)
    }
}
